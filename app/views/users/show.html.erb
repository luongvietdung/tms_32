<div class="row user-profile">
  <div class="col-md-9">
    <div class="row">
      <div class="col-md-3">
        <%= image_tag @user.avatar.medium.url %>
        <% if @user.supervisor? %>
          <div class="label label-danger"><%= t ".supervisor" %></div>
        <% end %>
      </div>
      <div class="col-md-9">

            <div class="row information">
            <div class="col-md-12">
            <div class="col-md-3"><strong><%= t ".name" %></strong></div>
            <div class="col-md-9"><%= @user.name %></div>
            </div>

            <div class="col-md-12">
            <div class="col-md-3"><strong><%= t ".email" %></strong></div>
            <div class="col-md-9">@user.email</div>
            </div>
            </div>

        <div class="row course-listing">
